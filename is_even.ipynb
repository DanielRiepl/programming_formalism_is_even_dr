    "def is_even(x):\n",
    "    if not isinstance(x, int):\n",
    "        raise TypeError(\"Only integers are allowed\")\n",
    "    try: \n",
    "        isinstance(x, int)\n",
    "    except TypeError:\n",
    "        raise TypeError(\"Input is not an integer\")\n",
    "    return (x % 2 == 0)\n",
